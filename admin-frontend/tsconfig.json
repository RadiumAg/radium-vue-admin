{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "sourceMap": true,
    "experimentalDecorators": true,
    "importsNotUsedAsValues": "error",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "types": [
      "./src/auto-imports",
      "./components",
      "unplugin-vue-define-options",
      "element-plus/global"
    ],
    "baseUrl": ".",
    "paths": {
      "@core/*": ["./src/core/*"],
      "@components": ["./src/pages/components/index.ts"],
      "@components/*": ["./src/pages/components/*"],
      "@pages/*": ["./src/pages/*"],
      "@assets/*": ["./src/assets/*"]
    }
  },
  "vueCompilerOptions": {
    "jsxTemplates": false
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/pages/components-example/upload/hash.ts"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
